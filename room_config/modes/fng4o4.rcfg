# fng1o1 room config

map AliveFNG
player_slots 8
scorelimit 200
timelimit 10
kill_delay 20

# room votes
add_vote " " "info"
add_vote "=== 房间设置 ===" "info"
add_vote "重开 Restart" "restart 10"

# rulesets
add_vote "规则: 10min 200points" "scorelimit 200;timelimit 10"
add_vote "规则: 15min 300points" "scorelimit 300;timelimit 15"
add_vote "规则: 20min 500points" "scorelimit 500;timelimit 20"
exec "room_config/modes/fngsuperconfig.rcfg"

# add mode switch votes
exec "room_config/modes.rcfg"